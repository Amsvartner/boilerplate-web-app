    "@mui/material": "^5.11.5",
    "@vitejs/plugin-react": "^4.2.1",
    "aws-amplify": "^6.0.10",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "match-sorter": "^6.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.7.0",
